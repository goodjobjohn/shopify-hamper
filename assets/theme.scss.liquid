  .actual-price {
    margin: 15px;
    top: -4px;
    position: relative;
    font-weight: bold;
  }
}

.custom-hamper { 
  padding: 0;
  margin: 0 0 20 0;
  list-style: none;
  
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  
  -webkit-flex-flow: row wrap;
  justify-content: center;
  
  .cat-heading {
    width: 100%;
    text-align: center;
    margin: 10px 0;
    font-weight: bold;
    letter-spacing: 3px;
    font-size: smaller;
  }

  input[type=checkbox] {
	  display:none;
  }

  input[type=checkbox] + label {
    height: 117px;
    width: 232px;
    display:inline-block;
    padding: 0 0 0 0px;
    //line-height: 260px;
    text-align: center;
    margin: 0px 10px 50px;
    position: relative;
    font-size: 10px;
    background-image: url(https://cdn.shopify.com/s/files/1/2221/3997/files/missing-thumb.jpg?731003146616314466);
    background-size: cover;
    
    span {
      display: block;
      margin-top: 122px;
    }
    &:hover {
      background-blend-mode: luminosity;
      background-color: black;
      
      &:before {
        content: "";
        position: absolute;
        height: 100%;
        width: 100%;
        background: rgba(234, 234, 234, 0.36); /* THIS IS WHAT EVER OVERLAY COLOUR YOU WANT */
        left: 0;
      }
    }
  }

  input[type=checkbox]:checked + label {
    background-image: url("https://cdn.shopify.com/s/files/1/2221/3997/files/wine.jpg?11469258297624535806");
    background-color: black; 
    background-blend-mode: luminosity;
    //border: 1px solid black;
    display:inline-block;
    padding: 0 0 0 0px;
    
    .checkmark {
      position: absolute;
      width: 22px;
      height:22px;
      -ms-transform: rotate(45deg); /* IE 9 */
      -webkit-transform: rotate(45deg); /* Chrome, Safari, Opera */
      transform: rotate(45deg);
      top: -94px;
      left: 120px;
    }

    .checkmark:before {
        content: "";
        position: absolute;
        width:20px;
        height:55px;
        background-color:#afd600;
        left:11px;
        top:0px;
    }

    .checkmark:after {
        content:"";
        position: absolute;
        width:21px;
        height:20px;
        background-color:#afd600;
        left:-2px;
        top:35px;
    }
  }
}
